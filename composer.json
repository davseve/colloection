{
    "name": "davids/collection",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Davids\\Collection\\": "src/"
        }
    },
    "authors": [
        {
            "name": "David Seveloff",
            "email": "davids@elementor.com"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^10",
